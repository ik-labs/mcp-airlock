{
  "dashboard": {
    "id": null,
    "title": "MCP Airlock Performance Dashboard",
    "tags": ["mcp", "airlock", "performance"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Request Latency",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(airlock_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P95 Latency",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.99, rate(airlock_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P99 Latency",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.05},
                {"color": "red", "value": 0.06}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Request Throughput",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(airlock_requests_total[5m]) * 60",
            "legendFormat": "Requests/min",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 1000},
                {"color": "green", "value": 2000}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Memory Usage",
        "type": "timeseries",
        "targets": [
          {
            "expr": "airlock_memory_usage_bytes / 1024 / 1024",
            "legendFormat": "Memory Usage (MB)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "decbytes",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 200000000},
                {"color": "red", "value": 400000000}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Active Connections",
        "type": "timeseries",
        "targets": [
          {
            "expr": "airlock_active_connections",
            "legendFormat": "Active Connections",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 5,
        "title": "Authentication Events",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(airlock_auth_events_total{result=\"success\"}[5m])",
            "legendFormat": "Successful Auth",
            "refId": "A"
          },
          {
            "expr": "rate(airlock_auth_events_total{result=\"failure\"}[5m])",
            "legendFormat": "Failed Auth",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 6,
        "title": "Policy Decisions",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(airlock_policy_decisions_total{decision=\"allow\"}[5m])",
            "legendFormat": "Allow",
            "refId": "A"
          },
          {
            "expr": "rate(airlock_policy_decisions_total{decision=\"deny\"}[5m])",
            "legendFormat": "Deny",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      },
      {
        "id": 7,
        "title": "Error Rate",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(airlock_errors_total[5m])",
            "legendFormat": "{{component}} - {{error_type}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.1},
                {"color": "red", "value": 1}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24}
      },
      {
        "id": 8,
        "title": "Upstream Call Duration",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(airlock_upstream_duration_seconds_bucket[5m]))",
            "legendFormat": "P95 Upstream Latency",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(airlock_upstream_duration_seconds_bucket[5m]))",
            "legendFormat": "P50 Upstream Latency",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24}
      },
      {
        "id": 9,
        "title": "Garbage Collection",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(airlock_gc_duration_seconds_sum[5m]) / rate(airlock_gc_duration_seconds_count[5m])",
            "legendFormat": "Average GC Duration",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(airlock_gc_duration_seconds_bucket[5m]))",
            "legendFormat": "P95 GC Duration",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.05},
                {"color": "red", "value": 0.1}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 32}
      },
      {
        "id": 10,
        "title": "Goroutines",
        "type": "timeseries",
        "targets": [
          {
            "expr": "airlock_goroutines_total",
            "legendFormat": "Goroutines",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 500},
                {"color": "red", "value": 1000}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 32}
      },
      {
        "id": 11,
        "title": "Data Redactions",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(airlock_redactions_total[5m])",
            "legendFormat": "{{pattern}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps"
          }
        },
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 40}
      }
    ],
    "templating": {
      "list": [
        {
          "name": "tenant",
          "type": "query",
          "query": "label_values(airlock_requests_total, tenant)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*"
        },
        {
          "name": "tool",
          "type": "query",
          "query": "label_values(airlock_requests_total{tenant=~\"$tenant\"}, tool)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*"
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "prometheus",
          "expr": "changes(airlock_requests_total[1m]) > 0",
          "titleFormat": "Deployment",
          "textFormat": "New deployment detected"
        }
      ]
    }
  }
}